{
  "name": "portfolio-ui",
  "version": "1.0.0",
  "description": "personal portfolio-ui React project",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run lint && webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.config.js",
    "format": "prettier --config .prettierrc.json  --write \"{public,src}/**/*.{ts,tsx,js}\"",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json",
    "lint:css": "stylelint --config .stylelintrc.json -- \"public/src/**/*.tsx\"",
    "lint": "npm run-script format && npm run-script lint:ts && npm run-script lint:css",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marianamn/portfolio-ui.git"
  },
  "author": "marianamn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marianamn/portfolio-ui/issues"
  },
  "homepage": "https://github.com/marianamn/portfolio-ui#readme",
  "peerDependencies": {
    "stylelint": ">= 9.1 < 10"
  },
  "devDependencies": {
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.2",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.14.3",
    "styled-components": "^3.4.9",
    "styled-icons": "^3.0.0",
    "stylelint": "^9.5.0",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scroll-to-component": "^1.0.2",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "tslint-immutable": "^4.7.0"
  },
  "engines": {
    "node": "8.1.3"
  }
}
